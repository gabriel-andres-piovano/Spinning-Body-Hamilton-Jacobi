      RowBox[{"KKgfun", "[", 
       RowBox[{"a", ",", "p", ",", "e", ",", "xg"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"r3g", "=", 
      RowBox[{
       FractionBox["1", 
        RowBox[{"1", "-", 
