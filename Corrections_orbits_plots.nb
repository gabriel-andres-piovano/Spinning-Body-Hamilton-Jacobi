    RowBox[{
     RowBox[{"r1g", "=", 
      FractionBox["p", 
       RowBox[{"1", "-", "e"}]]}], ";", "\[IndentingNewLine]", 
     RowBox[{"r2g", "=", 
      FractionBox["p", 
       RowBox[{"1", "+", "e"}]]}], ";", "\[IndentingNewLine]", 
     RowBox[{"z1g", "=", 
      SqrtBox[
       RowBox[{"1", "-", 
