       RowBox[{"a", ",", "p", ",", "e", ",", "xg"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"r2savgres", "=", 
      RowBox[{"r2savgFC", "[", 
       RowBox[{"a", ",", "p", ",", "e", ",", "xg"}], "]"}]}], ";", 
