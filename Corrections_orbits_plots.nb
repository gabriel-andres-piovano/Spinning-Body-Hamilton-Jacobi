       RowBox[{
        FractionBox["1", "2"], " ", 
        RowBox[{"(", 
         RowBox[{"r1g", "-", "r2g"}], ")"}], " ", 
        RowBox[{"Sin", "[", "\[Chi]r", "]"}]}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"Yrg", "=", 
      SqrtBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"EEg", "^", "2"}]}], ")"}], " ", 
