  RowBox[{"\[CapitalUpsilon]tsr2funFC", "[", 
   RowBox[{"a_", ",", "p_", ",", "e_", ",", "xg_", ",", "prec_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
