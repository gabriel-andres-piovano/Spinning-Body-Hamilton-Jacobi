Cell[BoxData[
 RowBox[{
  RowBox[{"EEgfun", "[", 
   RowBox[{"a_", ",", "p_", ",", "e_", ",", "xg_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "r1g", ",", "r2g", ",", "z1g", ",", "ddinc", ",", "ffinc", ",", "gginc", 
      ",", "hhinc", ",", "\[Kappa]g", ",", "\[Epsilon]g", ",", "\[Rho]g", ",",
       "\[Eta]g", ",", "\[Sigma]g", ",", "\[CapitalDelta]r1g", ",", 
      "\[CapitalDelta]r2g", ",", "EEgnum", ",", "EEgden", ",", "EEgdisc"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"r1g", "=", 
      FractionBox["p", 
       RowBox[{"1", "-", "e"}]]}], ";", "\[IndentingNewLine]", 
     RowBox[{"r2g", "=", 
      FractionBox["p", 
       RowBox[{"1", "+", "e"}]]}], ";", "\[IndentingNewLine]", 
     RowBox[{"z1g", "=", 
      SqrtBox[
       RowBox[{"1", "-", 
        RowBox[{"xg", "^", "2"}]}]]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"ddinc", "[", "r_", "]"}], ":=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"r", "^", "2"}], "-", 
         RowBox[{"2", "r"}], "+", 
         RowBox[{"a", "^", "2"}]}], ")"}], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"r", "^", "2"}], "+", 
         RowBox[{
          RowBox[{"a", "^", "2"}], " ", 
          RowBox[{"z1g", "^", "2"}]}]}], ")"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"ffinc", "[", "r_", "]"}], ":=", 
      RowBox[{
       RowBox[{"r", "^", "4"}], "+", 
       RowBox[{
        RowBox[{"a", "^", "2"}], 
        RowBox[{"(", 
         RowBox[{"r", "+", "2"}], ")"}], "r"}], "+", 
       RowBox[{
        RowBox[{"a", "^", "2"}], " ", 
        RowBox[{"z1g", "^", "2"}], " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"r", "^", "2"}], "-", 
          RowBox[{"2", "r"}], "+", 
          RowBox[{"a", "^", "2"}]}], ")"}]}]}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{
      RowBox[{"gginc", "[", "r_", "]"}], ":=", 
      RowBox[{"2", " ", "a", " ", "r"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"hhinc", "[", "r_", "]"}], ":=", 
      RowBox[{
       RowBox[{"r", 
        RowBox[{"(", 
         RowBox[{"r", "-", "2"}], ")"}]}], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"r", "^", "2"}], "-", 
          RowBox[{"2", "r"}], "+", 
          RowBox[{"a", "^", "2"}]}], ")"}], 
        RowBox[{
         RowBox[{"z1g", "^", "2"}], "/", 
